<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="1.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="1.0" />
      <RowConstraints />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints minWidth="10.0" prefWidth="1.0" />
      <ColumnConstraints minWidth="10.0" prefWidth="30.0" />
      <ColumnConstraints minWidth="10.0" prefWidth="30.0" />
      <ColumnConstraints minWidth="10.0" prefWidth="30.0" />
      <ColumnConstraints minWidth="10.0" prefWidth="30.0" />
      <ColumnConstraints minWidth="10.0" prefWidth="30.0" />
      <ColumnConstraints minWidth="10.0" prefWidth="1.0" />
   </columnConstraints>
   <children>
      <Button fx:id="buttonZero" maxWidth="70.0" mnemonicParsing="false" onAction="#zero" text="0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="7" />
      <Button fx:id="buttonDot" mnemonicParsing="false" onAction="#dot" prefWidth="30.0" text="," GridPane.columnIndex="3" GridPane.rowIndex="7" />
      <Button fx:id="buttonSubtract" mnemonicParsing="false" onAction="#subtract" prefWidth="30.0" text="-" GridPane.columnIndex="4" GridPane.rowIndex="6" />
      <Button fx:id="buttonAdd" mnemonicParsing="false" onAction="#add" prefWidth="30.0" text="+" GridPane.columnIndex="4" GridPane.rowIndex="7" />
      <Button fx:id="buttonMultiply" mnemonicParsing="false" onAction="#multiply" prefWidth="30.0" text="*" GridPane.columnIndex="4" GridPane.rowIndex="5" />
      <Button fx:id="buttonSplit" mnemonicParsing="false" onAction="#split" prefWidth="30.0" text="/" GridPane.columnIndex="4" GridPane.rowIndex="4" />
      <Button fx:id="buttonPlusMinus" mnemonicParsing="false" onAction="#plusMinus" text="+/-" GridPane.columnIndex="4" GridPane.rowIndex="3">
         <font>
            <Font size="10.0" />
         </font></Button>
      <Button fx:id="buttonClear" mnemonicParsing="false" onAction="#clear" prefWidth="30.0" text="C" GridPane.columnIndex="2" GridPane.rowIndex="3" />
      <Button fx:id="buttonAbsoluteValue" mnemonicParsing="false" onAction="#absoluteValue" text="|x|" GridPane.columnIndex="3" GridPane.rowIndex="3" />
      <Button fx:id="buttonUndo" mnemonicParsing="false" onAction="#undo" prefWidth="30.0" text="←" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="buttonRoot" mnemonicParsing="false" onAction="#root" prefWidth="30.0" text="√" GridPane.columnIndex="5" GridPane.rowIndex="3" />
      <Button fx:id="buttonPercent" mnemonicParsing="false" onAction="#percent" prefWidth="30.0" text="\%" GridPane.columnIndex="5" GridPane.rowIndex="4" />
      <Button fx:id="buttonInverse" mnemonicParsing="false" onAction="#inverse" text="1/x" GridPane.columnIndex="5" GridPane.rowIndex="5">
         <font>
            <Font size="10.0" />
         </font></Button>
      <Button fx:id="buttonEqual" mnemonicParsing="false" onAction="#equal" prefHeight="65.0" prefWidth="30.0" text="=" GridPane.columnIndex="5" GridPane.rowIndex="6" GridPane.rowSpan="2" GridPane.valignment="CENTER" />
      <Button fx:id="buttonSeven" mnemonicParsing="false" onAction="#seven" prefWidth="30.0" text="7" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Button fx:id="buttonFour" mnemonicParsing="false" onAction="#four" prefWidth="30.0" text="4" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <Button fx:id="buttonOne" mnemonicParsing="false" onAction="#one" prefWidth="30.0" text="1" GridPane.columnIndex="1" GridPane.rowIndex="6" />
      <Button fx:id="buttonEight" mnemonicParsing="false" onAction="#eight" prefWidth="30.0" text="8" GridPane.columnIndex="2" GridPane.rowIndex="4" />
      <Button fx:id="buttonNine" mnemonicParsing="false" onAction="#nine" prefWidth="30.0" text="9" GridPane.columnIndex="3" GridPane.rowIndex="4" />
      <Button fx:id="buttonFive" mnemonicParsing="false" onAction="#five" prefWidth="30.0" text="5" GridPane.columnIndex="2" GridPane.rowIndex="5" />
      <Button fx:id="buttonSix" mnemonicParsing="false" onAction="#six" prefWidth="30.0" text="6" GridPane.columnIndex="3" GridPane.rowIndex="5" />
      <Button fx:id="buttonTwo" mnemonicParsing="false" onAction="#two" prefWidth="30.0" text="2" GridPane.columnIndex="2" GridPane.rowIndex="6" />
      <Button fx:id="buttonThree" mnemonicParsing="false" onAction="#three" prefWidth="30.0" text="3" GridPane.columnIndex="3" GridPane.rowIndex="6" />
      <TextField fx:id="textFieldDisplay" alignment="CENTER_RIGHT" editable="false" prefHeight="52.0" prefWidth="190.0" GridPane.columnIndex="1" GridPane.columnSpan="5" GridPane.rowIndex="1" GridPane.rowSpan="2" />
   </children>
</GridPane>
